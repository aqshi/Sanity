# platform :ios, '9.0'
use_frameworks!

target 'Sanity' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  
  pod 'Firebase'
  pod 'GoogleSignIn'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Charts'
  # Pods for Sanity
  target 'SanityUnitTests' do
      inherit! :search_paths
      pod 'Firebase'
  end
end

